SER',\n          user: null,\n        });\n      }\n    });\n    return() => {\n      unsubscribe();\n    }\n  }, []);\n\n  console.log(\"USER IS >>>\", user);\n  \n  return (\n    <Router>\n      <div className=\"app\">\n        <Switch>\n          <Route path='/checkout'>\n            <Header/>\n            <Menu/>\n            <Checkout/>\n            <Footer></Footer>\n          </Route>\n          <Route path='/login'>\n            <Login/>\n          </Route>\n\n          {/* Direccion raiz de la pagina,es decir, la pagina principal  */}\n          <Route path='/'>\n            <Header/>\n            <Menu/>\n            <Home/>\n            <Footer/>\n          </Route>\n        </Switch>\n      </div>\n    </Router> \n  );\n}\n\nexport default App;\n","C:\\Users\\user\\Desktop\\Arquitectura Fronted\\React\\amazon-clone\\src\\reportWebVitals.js",[],"C:\\Users\\user\\Desktop\\Arquitectura Fronted\\React\\amazon-clone\\src\\Header.js",[],"C:\\Users\\user\\Desktop\\Arquitectura Fronted\\React\\amazon-clone\\src\\Home.js",[],"C:\\Users\\user\\Desktop\\Arquitectura Fronted\\React\\amazon-clone\\src\\Product.js",["75"],"import React from 'react';\r\nimport './Product.css';\r\nimport {useStateValue} from './StateProvider';\r\n\r\nfunction Product({id, title, image, price, rating}) {\r\n\r\n    const [{basket} , dispatch] = useStateValue();\r\n\r\n    const addToBasket = () => {\r\n        dispatch({\r\n            type: 'ADD_TO_BASKET',\r\n            item: {\r\n                id: id,\r\n                title: title,\r\n                image: image,\r\n                price: price, \r\n                rating: rating,\r\n            },\r\n        });\r\n    };\r\n\r\n    return (\r\n        <div className='product'>\r\n            <div className=\"product__info\">\r\n                <p>{title}</p>\r\n                <p className=\"product__price\">\r\n                    <small>$</small>\r\n                    <strong>{price}</strong>\r\n                </p>\r\n    